@model Int.MobileUI.Models.SubporecessModel
@{
    var temp = Model;
}
<input hidden type="text" value="@temp.State" />
<link rel="stylesheet" href="/assets/css/jquery-ui-1.10.3.custom.min.css" />
<link rel="stylesheet" href="/assets/css/chosen.css" />
<div class="widget-body">
    <div class="widget-main">
        <div id="fuelux-wizard" class="row-fluid" data-target="#step-container">
            <ul class="wizard-steps">
                <li data-target="#step1" class="active">
                    <span class="step">1</span>
                    <span class="title">教研中心提案责任人</span>
                </li>

                <li data-target="#step2">
                    <span class="step">2</span>
                    <span class="title">课程讲师备课</span>
                </li>

                <li data-target="#step3">
                    <span class="step">3</span>
                    <span class="title">课件提交提案责任人</span>
                </li>

                <li data-target="#step4">
                    <span class="step">4</span>
                    <span class="title">课件提交经手人</span>
                </li>

                <li data-target="#step5">
                    <span class="step">5</span>
                    <span class="title">经手人提交课件给客户</span>
                </li>

                <li data-target="#step6">
                    <span class="step">6</span>
                    <span class="title">提交运营中心</span>
                </li>
            </ul>
        </div>

        <div class="step-content row-fluid position-relative" id="step-container">
            <div class="step-pane active" id="step1">
                <div class="space-8"></div>
                <input class="ace" type="checkbox"/>
                <span class="lbl"> 请经手人了解客户的实际情况及对本次课程的其他需求 </span>
                <div class="space-8"></div>
                <input class="ace" type="checkbox" />
                <span class="lbl"> 与讲师说明客户现状及客户对本次课程的其他需求 </span>
            </div>
            <div class="step-pane" id="step2">
                <div class="form-group">
                    <div class="space-8"></div>
                    <label class="col-sm-2 control-label no-padding-right">课程讲师备课</label>&nbsp;&nbsp;
                    <input class="ace" type="checkbox" />
                    <span class="lbl"> 提案负责人跟进讲师备课情况，确认课件发送时间 </span>
                </div>
            </div>
            <div class="step-pane" id="step3">
                <div class="form-group">
                    <label class="col-sm-2 control-label no-padding-right">提交课件给提案责任人</label>&nbsp;&nbsp;
                </div>
            </div>
            <div class="step-pane" id="step4">
                <div class="form-group">
                    <label class="col-sm-2 control-label no-padding-right">课件提交经手人</label>&nbsp;&nbsp;
                </div>
            </div>
            <div class="step-pane" id="step5">
                <div class="form-group">
                    <label class="col-sm-2 control-label no-padding-right">经手人提交课件给客户</label>&nbsp;&nbsp;
                    <button class="btn">
                        调整<i class="icon-arrow-right icon-on-right"></i>
                    </button>
                </div>
            </div>
            <div class="step-pane" id="step6">
                <input class="ace" type="checkbox" />
                <span class="lbl"> 提交运营中心 </span>
                <div style="display:none"></div>
            </div>
        </div>
        <div class="row-fluid wizard-actions">
            <button class="btn">
                更新流程<i class="icon-arrow-right icon-on-right"></i>
            </button>
            <button class="btn btn-prev">
                <i class="icon-arrow-left"></i>
                Prev
            </button>

            <button class="btn btn-success btn-next" data-last="Finish ">
                Next
                <i class="icon-arrow-right icon-on-right"></i>
            </button>
        </div>
    </div><!-- /widget-main -->
</div><!-- /widget-body -->

